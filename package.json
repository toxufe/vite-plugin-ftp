    "format": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "ftp-deploy": "^2.4.7",
    "ora": "^6.1.2",
    "typescript": "^4.8.4",
    "vite": "^2.5.10"
  },
  "devDependencies": {
    "vue": "^3.2.4",
    "@types/node": "^18.11.9",
    "tsup": "^6.3.0"
  }
