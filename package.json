  "repository": {
    "type": "git",
    "url": "git+https://github.com/JianGuoPaPa/vite-plugin-ftp"
  },
  "keywords": [
    "vite",
    "vue3",
    "vue-next",
    "ftp",
    "deploy"
  ],
  "author": "jianguopapa (https://github.com/jianguopapa)",
  "license": "ISC",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
